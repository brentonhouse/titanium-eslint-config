{
	"name": "@titanium/eslint-config",
	"version": "0.0.11",
	"description": "ESLint configuration for Titanium native mobile app projects",
	"main": "index.js",
	"scripts": {
		"list-files": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"test": "npm install --ignore-scripts && npm run list-files",
		"prepublishOnly": "git pull && bump"
	},
	"author": {
		"name": "Brenton House",
		"email": "brenton.house@gmail.com",
		"url": "https://brenton.house"
	},
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-eslint-config"
	},
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-eslint-config/issues"
	},
	"license": "MIT",
	"dependencies": {
		"@geek/eslint-config": "0.0.9"
	},
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"eslint",
		"turbo",
		"axway",
		"config",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"brenton-house"
	],
	"titanium": {
		"ignore": true
	}
}
